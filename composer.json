{
  "name": "matucana/weather-bot",
  "description": "Weather bot sending notifications to vk.com",
  "type": "project",
  "homepage": "https://github.com/matucana/weather-bot",
  "license": "MIT",
  "support": {
    "email": "victor.dialogbox@gmail.com"
  },
  "authors": [
    {
      "name": "Vinogradov Victor",
      "email": "victor.dialogbox@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.4",
    "symfony/dotenv": "5.2.*",
    "vkcom/vk-php-sdk": "5.101.*",
    "cmfcmf/openweathermap-php-api": "3.3.*",
    "http-interop/http-factory-guzzle": "1.0.*",
    "php-http/guzzle6-adapter": "2.0.*",
    "php-di/php-di": "^6.3",
    "monolog/monolog": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "Matucana\\WeatherBot\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Matucana\\WeatherBot\\Test\\": "tests/"
    }
  }
}
